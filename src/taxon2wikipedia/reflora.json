    "temComoSinonimo": "<span><a onclick=\"carregaTaxonGrupo(604224)\" href=\"javascript:void(0);\"><div class=\"tipoSinonimo\"> basionym</div><div class=\"sinonimo\"> <i>Calathea wiotiana</i></div><div class=\"nomeAutorSinonimo\"> Jacob-Makoy ex \u00c9.Morren</div></a></span><br><span><a onclick=\"carregaTaxonGrupo(595407)\" href=\"javascript:void(0);\"><div class=\"tipoSinonimo\"> homotype</div><div class=\"sinonimo\"> <i>Calathea wioti</i></div><div class=\"nomeAutorSinonimo\"> (E. Morren) Regel</div></a></span><br><span><a onclick=\"carregaTaxonGrupo(9325)\" href=\"javascript:void(0);\"><div class=\"tipoSinonimo\"> homotype</div><div class=\"sinonimo\"> <i>Calathea wiotii</i></div><div class=\"nomeAutorSinonimo\"> (E.Morren) Regel</div></a></span><br><span><a onclick=\"carregaTaxonGrupo(139200)\" href=\"javascript:void(0);\"><div class=\"tipoSinonimo\"> homotype</div><div class=\"sinonimo\"> <i>Goeppertia wiotii</i></div><div class=\"nomeAutorSinonimo\"> (\u00c9.Morren) Borchs. & S.Su\u00e1rez</div></a></span><br><span><a onclick=\"carregaTaxonGrupo(139201)\" href=\"javascript:void(0);\"><div class=\"tipoSinonimo\"> homotype</div><div class=\"sinonimo\"> <i>Maranta wiotii</i></div><div class=\"nomeAutorSinonimo\"> E.Morren</div></a></span><br>",
