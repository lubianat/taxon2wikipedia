    "temComoSinonimo": "<span><a onclick=\"carregaTaxonGrupo(43353)\" href=\"javascript:void(0);\"><div class=\"tipoSinonimo\"> basi\u00f4nimo</div><div class=\"sinonimo\"> <i>Panax calvum</i></div><div class=\"nomeAutorSinonimo\"> Cham.</div></a></span><br><span><a onclick=\"carregaTaxonGrupo(43250)\" href=\"javascript:void(0);\"><div class=\"tipoSinonimo\"> heterot\u00edpico</div><div class=\"sinonimo\"> <i>Didymopanax claussenianus</i></div><div class=\"nomeAutorSinonimo\"> Decne. & Planch. ex Marchal</div></a></span><br><span><a onclick=\"carregaTaxonGrupo(43265)\" href=\"javascript:void(0);\"><div class=\"tipoSinonimo\"> heterot\u00edpico</div><div class=\"sinonimo\"> <i>Didymopanax micranthus</i></div><div class=\"nomeAutorSinonimo\"> Marchal</div></a></span><br><span><a onclick=\"carregaTaxonGrupo(43270)\" href=\"javascript:void(0);\"><div class=\"tipoSinonimo\"> heterot\u00edpico</div><div class=\"sinonimo\"> <i>Didymopanax pachycarpus</i></div><div class=\"nomeAutorSinonimo\"> Marchal</div></a></span><br><span><a onclick=\"carregaTaxonGrupo(5138)\" href=\"javascript:void(0);\"><div class=\"tipoSinonimo\"> heterot\u00edpico</div><div class=\"sinonimo\"> <i>Schefflera clausseniana</i></div><div class=\"nomeAutorSinonimo\"> (Decne. & Planch. ex Marchal) Frodin</div></a></span><br><span><a onclick=\"carregaTaxonGrupo(26558)\" href=\"javascript:void(0);\"><div class=\"tipoSinonimo\"> heterot\u00edpico</div><div class=\"sinonimo\"> <i>Schefflera pachycarpa</i></div><div class=\"nomeAutorSinonimo\"> (Marchal) Frodin</div></a></span><br><span><a onclick=\"carregaTaxonGrupo(43380)\" href=\"javascript:void(0);\"><div class=\"tipoSinonimo\"> heterot\u00edpico</div><div class=\"sinonimo\"> <i>Schefflera regnelliana</i></div><div class=\"nomeAutorSinonimo\"> Frodin</div></a></span><br><span><a onclick=\"carregaTaxonGrupo(5136)\" href=\"javascript:void(0);\"><div class=\"tipoSinonimo\"> homot\u00edpico</div><div class=\"sinonimo\"> <i>Schefflera calva</i></div><div class=\"nomeAutorSinonimo\"> (Cham.) Frodin & Fiaschi</div></a></span><br>",
